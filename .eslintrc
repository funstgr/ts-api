{
    "extends": [ 
      "airbnb-typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "plugins": [
      "json-format",
      "@typescript-eslint"
    ],
    "settings": {
      "json/sort-package-json": false
    },
    "rules": {
      "import/extensions": 0,
      "noImplicitAny": 0
      
    }
  }